<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-configuration PUBLIC
"-//Hibernate/Hibernate Configuration DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-configuration-3.0.dtd">
<hibernate-configuration>
    <session-factory>
        <property name="hibernate.bytecode.use_reflection_optimizer">false</property>
        <property name="hibernate.connection.driver_class">com.mysql.jdbc.Driver</property>
        <property name="hibernate.connection.url">jdbc:mysql://localhost:3306/mobilestore?characterEncoding=UTF-8</property>
        <property name="hibernate.connection.username">root</property>
        <property name="hibernate.dialect">org.hibernate.dialect.MySQLDialect</property>
        <property name="hibernate.search.autoregister_listeners">false</property>
        <property name="hibernate.validator.apply_to_ddl">false</property>
        <mapping resource="Model/Customergroup.hbm.xml" />
        <mapping resource="Model/Warehouse.hbm.xml" />
        <mapping resource="Model/Supliergroup.hbm.xml" />
        <mapping resource="Model/Producer.hbm.xml" />
        <mapping resource="Model/Infomail.hbm.xml" />
        <mapping resource="Model/Productdetail.hbm.xml" />
        <mapping resource="Model/Customer.hbm.xml" />
        <mapping resource="Model/Bill.hbm.xml" />
        <mapping resource="Model/Receiptandpayment.hbm.xml" />
        <mapping resource="Model/Suplier.hbm.xml" />
        <mapping resource="Model/Warehousedetail.hbm.xml" />
        <mapping resource="Model/Product.hbm.xml" />
        <mapping resource="Model/Productgroup.hbm.xml" />
        <mapping resource="Model/Billdetail.hbm.xml" />
        <mapping resource="Model/Info.hbm.xml" />
        <mapping resource="Model/Billtemplate.hbm.xml" />
        <mapping resource="Model/Employee.hbm.xml" />
        <mapping resource="Model/User.hbm.xml" />
        <mapping resource="Model/Producergroup.hbm.xml" />
    </session-factory>
</hibernate-configuration>
